{
	"name": "oktolity",
	"version": "0.1.0",
	"private": true,
	"description": "T3 Stack (Next.js, Typescript, tRPC, Prisma, Tailwind CSS, NextAuth.js) application to manage repositories, starred on GitHub",
	"keywords": [
		"next",
		"typescript",
		"trpc",
		"prisma",
		"tailwindcss",
		"nextauth"
	],
	"homepage": "https://github.com/Constantiner/oktolity#readme",
	"bugs": {
		"url": "https://github.com/Constantiner/oktolity/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Constantiner/oktolity.git"
	},
	"license": "MIT",
	"author": {
		"name": "Konstantin Kovalev",
		"email": "constantiner@gmail.com"
	},
	"type": "module",
	"scripts": {
		"build": "npx prisma migrate deploy && npm run build:dev",
		"build:dev": "npx next build",
		"db:push": "npx prisma db push",
		"db:studio": "npx prisma studio",
		"dev": "npx next dev",
		"format": "npx prettier --write '**/*.{js,cjs,mjs,ts,jsx,tsx,css,scss,json,htm,html,yml,lintstagedrc,stylelintrc}'",
		"postinstall": "npx prisma generate",
		"lint": "npx tsc --noEmit && npm run lint:only",
		"lint-staged": "npx lint-staged",
		"lint:config-inspector": "npx @eslint/config-inspector",
		"lint:only": "npx eslint . --report-unused-disable-directives --max-warnings 0",
		"prepare": "husky",
		"start": "npx next start"
	},
	"dependencies": {
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^5.16.1",
