		"@eslint/config-inspector": "^0.5.1",
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.6.0",
		"@next/eslint-plugin-next": "^14.2.4",
		"@stylistic/eslint-plugin-js": "^2.3.0",
