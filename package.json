{
	"name": "oktolity",
	"version": "0.1.0",
	"private": true,
	"description": "T3 Stack (Next.js, Typescript, tRPC, Prisma, Tailwind CSS, NextAuth.js) application to manage repositories, starred on GitHub",
	"keywords": [
		"next",
		"typescript",
		"trpc",
		"prisma",
		"tailwindcss",
		"nextauth"
	],
	"homepage": "https://github.com/Constantiner/oktolity#readme",
	"bugs": {
		"url": "https://github.com/Constantiner/oktolity/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Constantiner/oktolity.git"
	},
	"license": "MIT",
	"author": {
		"name": "Konstantin Kovalev",
		"email": "constantiner@gmail.com"
	},
	"type": "module",
	"scripts": {
		"build": "npx prisma migrate deploy && npx next build",
		"build:dev": "npx next build",
		"db:push": "npx prisma db push",
		"db:studio": "npx prisma studio",
		"dev": "npx next dev --turbo",
		"format": "npx prettier --write '**/*.{js,cjs,mjs,ts,jsx,tsx,css,scss,json,htm,html,yml,lintstagedrc,stylelintrc}'",
		"postinstall": "npx prisma generate",
		"lint": "npx tsc --noEmit && npx eslint . --report-unused-disable-directives --max-warnings 0",
		"lint-staged": "npx lint-staged",
		"lint:config-inspector": "npx @eslint/config-inspector",
		"prepare": "husky",
		"start": "npx next start"
	},
	"dependencies": {
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^5.13.0",
		"@t3-oss/env-nextjs": "^0.10.1",
		"@tanstack/react-query": "^5.32.0",
		"@trpc/client": "next",
		"@trpc/react-query": "next",
		"@trpc/server": "next",
		"next": "^14.2.3",
		"next-auth": "^4.24.7",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"server-only": "^0.0.1",
		"superjson": "^2.2.1",
		"zod": "^3.23.5"
	},
	"devDependencies": {
		"@eslint/config-inspector": "^0.4.7",
		"@eslint/js": "^9.1.1",
		"@next/eslint-plugin-next": "^14.2.3",
		"@stylistic/eslint-plugin-js": "^1.7.2",
		"@types/eslint": "^8.56.10",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.12.7",
		"@types/react": "^18.3.1",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"dotenv-cli": "^7.4.1",
		"eslint": "^8.57.0",
		"eslint-config-next": "^14.2.3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^17.4.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.6",
		"eslint-plugin-security": "^3.0.0",
		"eslint-plugin-unicorn": "^52.0.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-packagejson": "^2.5.0",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"prisma": "^5.13.0",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.8.0"
	},
	"ct3aMetadata": {
		"initVersion": "7.32.0"
	}
}
